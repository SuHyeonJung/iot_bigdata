#include <stdio.h>

/*躍 OR쵖쟕킨
 * A촜썒	B촜썒	써쎀
 * 썐		썐	썐
 * 츂		썐	츂
 * 썐		츂		츂
 * 츂		츂		츂
 */

void main(){
	int num1=1;
	int num2=0;
	int num3=100;
	int result=0;

	result = (!num1);
	printf("!num1 	=> 써쎀: %d\n", result);
	
	result = (!num2);
	printf("!num2 	=> 써쎀: %d\n", result);
	
	result = (!num3);
	printf("!num3 	=> 써쎀: %d\n", result);
}
